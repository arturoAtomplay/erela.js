{
  "name": "@arturoatomplay/erela.js",
  "version": "1.0.1",
  "description": "A fork of erela.js, An easy-to-use Lavalink client for NodeJS.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext .ts ./src",
    "publish:stable": "yarn build && yarn publish --access=public",
    "publish:beta": "yarn build && yarn publish --tag beta --access=public"
  },
  "keywords": [
    "lavalink",
    "discord",
    "music",
    "bot",
    "discord.js",
    "eris"
  ],
  "author": "MenuDocs (https://github.com/MenuDocs) - Fork (https://github.com/arturoAtomplay/erela.js)",
  "contributors": [
    "Solaris9",
    "Anish-Shobith",
    "ayntee"
  ],
  "license": "Apache-2.0",
  "repository": "arturoAtomplay/erela.js",
  "bugs": "https://github.com/arturoAtomplay/erela.js",
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@discordjs/collection": "^0.5.0",
    "petitio": "^1.4.0",
    "ws": "^8.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "homepage": "https://github.com/MenuDocs/erela.js#readme"
}
